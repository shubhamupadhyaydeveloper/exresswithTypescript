{
  "version": 2,
  "builds": [
    {
      "src": "src/app.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/dist/app.js"
    }
  ],
  "installCommand": "npm install",
  "buildCommand": "npm install && tsc && tsc-alias",
  "outputDirectory": "dist"
}
